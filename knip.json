{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "app/app.vue",
        "app/app.config.ts",
        "app/pages/**/*.vue",
        "app/components/**/*.vue",
        "app/composables/**/*.ts",
        "app/utils/**/*.ts",
        "app/constants/**/*.ts",
        "electron/main.ts",
        "electron/preload.ts",
        "electron/image/pdf-combine-worker.ts",
        "electron/ocr/worker/main.ts",
        "electron/search/worker.ts",
        "scripts/electron-run/**/*.ts"
    ],
    "project": [
        "app/**/*.{ts,vue}",
        "electron/**/*.ts",
        "scripts/electron-run/**/*.ts"
    ],
    "paths": {
        "@app/*": ["app/*"],
        "@electron/*": ["electron/*"]
    },
    "ignore": [
        "app/types/*.d.ts",
        "electron/types/utif.d.ts",
        ".nuxt/**"
    ],
    "ignoreDependencies": [
        "@iconify-json/circle-flags",
        "@iconify-json/lucide",
        "@babel/parser",
        "@iconify/utils",
        "@intlify/core",
        "@intlify/core-base",
        "@intlify/h3",
        "@intlify/message-compiler",
        "@intlify/shared",
        "@intlify/utils",
        "@vue/compiler-core",
        "@vue/compiler-dom",
        "@vue/compiler-ssr",
        "@vue/devtools-api",
        "@vue/reactivity",
        "@vue/runtime-core",
        "@vue/runtime-dom",
        "@vue/server-renderer",
        "@vue/shared",
        "consola",
        "cookie-es",
        "defu",
        "destr",
        "devalue",
        "entities",
        "estree-walker",
        "h3",
        "hookable",
        "iron-webcrypto",
        "node-mock-http",
        "@playwright/test",
        "playwright",
        "radix3",
        "sass-embedded",
        "source-map-js",
        "tailwindcss",
        "ufo",
        "uncrypto",
        "unhead",
        "vue-bundle-renderer"
    ],
    "ignoreBinaries": [
        "codesign"
    ]
}
